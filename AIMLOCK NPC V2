local a=game:GetService("Players")
local b=game:GetService("RunService")
local c=game:GetService("UserInputService")
local d=game:GetService("Workspace")
local e=a.LocalPlayer
local f=d.CurrentCamera
local g=false
local h
local i=nil
local j=false
local k={}
local l={}
for m,n in ipairs(d:GetDescendants()) do if n:IsA("Model") and n:FindFirstChild("HumanoidRootPart") and n:FindFirstChild("Humanoid") and not a:GetPlayerFromCharacter(n) then local o=n.HumanoidRootPart
local p=n.Humanoid
if k[o]==nil then k[o]={Size=o.Size,Transparency=o.Transparency,CanCollide=o.CanCollide}end
p.Died:Connect(function()task.wait(0.2)
local q=k[o]
if q then o.Size=q.Size
o.Transparency=q.Transparency
o.CanCollide=q.CanCollide end end)end end
local r=Instance.new("ScreenGui")
r.Name="hà nhân camlock"
r.ResetOnSpawn=false
r.Parent=e:WaitForChild("PlayerGui")
local s=Instance.new("Frame")
s.Size=UDim2.new(0,200,0,120)
s.Position=UDim2.new(0.85,-100,0.75,-60)
s.AnchorPoint=Vector2.new(0.5,0.5)
s.BackgroundColor3=Color3.fromRGB(30,30,30)
s.BackgroundTransparency=0.3
s.BorderSizePixel=0
s.Active=true
s.Draggable=true
s.Parent=r
local t=Instance.new("UIScale")
t.Scale=1/1.2
t.Parent=s
local u=Instance.new("UICorner")
u.CornerRadius=UDim.new(0,12)
u.Parent=s
local v=Instance.new("TextButton")
v.Size=UDim2.new(1,-20,0,40)
v.Position=UDim2.new(0,10,0,10)
v.BackgroundColor3=Color3.fromRGB(30,30,30)
v.BackgroundTransparency=0.4
v.Text="hà nhân: TẮT"
v.TextColor3=Color3.new(1,1,1)
v.Font=Enum.Font.Fantasy
v.TextSize=22
v.Parent=s
local w=Instance.new("UICorner")
w.CornerRadius=UDim.new(0,12)
w.Parent=v
v.MouseButton1Click:Connect(function()g=not g
if g then v.Text="HÀ NHÂN: MỞ"
h=b.RenderStepped:Connect(function()end)else v.Text="HÀ NHÂN: TẮT"
if h then h:Disconnect()h=nil end end end)
game:GetService("StarterGui"):SetCore("SendNotification",{Title="Hà nhân (quái)",Text="HUY ĐZ",Duration=10})
